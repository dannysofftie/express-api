{
    "name": "node-ts-app-scaffold",
    "version": "1.0.0",
    "description": "A Node.js Application Scaffold with TypeScript, to ease starting new projects.",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpileOnly --no-notify src/index.ts",
        "start": "npm run clean && npm run build && node dist/index.js",
        "build": "tsc -b",
        "clean": "tsc -b --clean",
        "debug": "node --inspect -r ts-node/register src/index.ts",
        "docker-build": "npm run docker-stop && docker-compose build && docker rmi -f $(docker images --quiet --filter 'dangling=true')",
        "docker-start": "docker-compose up",
        "docker-stop": "docker-compose down"
    },
    "keywords": [
        "scaffold",
        "nodejs",
        "typescript"
    ],
    "author": "Danny Sofftie",
    "license": "ISC",
    "dependencies": {
        "aws-sdk": "^2.392.0",
        "axios": "^0.18.0",
        "bcrypt": "^3.0.3",
        "body-parser": "^1.18.3",
        "braintree": "^2.15.0",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.4",
        "dotenv": "^6.1.0",
        "ejs": "^2.6.1",
        "express": "^4.16.4",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.24.0",
        "moment-duration-format": "^2.2.2",
        "mongoose": "^5.3.14",
        "multer": "^1.4.1",
        "multer-s3": "^2.9.0",
        "nodemailer": "^4.6.8",
        "serve-favicon": "^2.5.0",
        "ts-node-dev": "^1.0.0-pre.32",
        "typescript": "^3.2.4"
    },
    "engines": {
        "node": "11.5.0"
    },
    "repository": {
        "type": "git",
        "url": "git+git@github.com:dannysofftie/node-ts-app-scaffold.git"
    },
    "bugs": {
        "url": "https://github.com/dannysofftie/node-ts-app-scaffold.git/issues"
    },
    "homepage": "https://github.com/dannysofftie/node-ts-app-scaffold.git#readme",
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/body-parser": "^1.17.0",
        "@types/cookie-parser": "^1.4.1",
        "@types/cors": "^2.8.4",
        "@types/dotenv": "^4.0.3",
        "@types/ejs": "^2.6.0",
        "@types/express": "^4.16.0",
        "@types/mongoose": "^5.3.1",
        "@types/multer": "^1.3.7",
        "@types/multer-s3": "^2.7.6",
        "@types/node": "^11.11.3",
        "@types/nodemailer": "^4.6.5",
        "@types/serve-favicon": "^2.2.30",
        "prettier": "^1.16.4"
    }
}
